<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0
                      http://maven.apache.org/xsd/settings-1.0.0.xsd">

    <servers>
        <server>
            <id>internal</id>
            <username>${env.REPO_USER}</username>
            <password>${env.REPO_PASSWORD}</password>
        </server>
        <server>
            <id>SBBOL_thirdparty</id>
            <username>${env.REPO_USER}</username>
            <password>${env.REPO_PASSWORD}</password>
        </server>
        <server>
            <id>public</id>
            <username>${env.REPO_USER}</username>
            <password>${env.REPO_PASSWORD}</password>
        </server>
        <server>
            <id>thirdparty</id>
            <username>${env.REPO_USER}</username>
            <password>${env.REPO_PASSWORD}</password>
        </server>
    </servers>

    <profiles>
        <profile>

            <repositories>
                <repository>
                    <id>SBBOL_thirdparty</id>
                    <url>http://sbtatlas.sigma.sbrf.ru/nexus/content/repositories/SBBOL_thirdparty/</url>
                </repository>
            </repositories>

            <pluginRepositories>
                <pluginRepository>
                    <id>SBBOL_thirdparty</id>
                    <url>http://sbtatlas.sigma.sbrf.ru/nexus/content/repositories/SBBOL_thirdparty/</url>
                </pluginRepository>

                <pluginRepository>
                    <id>public</id>
                    <url>http://sbtatlas.sigma.sbrf.ru/nexus/content/groups/public/</url>
                </pluginRepository>

                <pluginRepository>
                    <id>thirdparty</id>
                    <url>http://sbtatlas.sigma.sbrf.ru/nexus/content/repositories/thirdparty</url>
                </pluginRepository>
            </pluginRepositories>

        </profile>
    </profiles>

    <mirrors>
        <mirror>
            <id>internal</id>
            <name>Mirror of central</name>
            <url>http://sbtatlas.sigma.sbrf.ru/nexus/content/groups/internal</url>
            <mirrorOf>*</mirrorOf>
        </mirror>
    </mirrors>
</settings>
